<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="500.0" prefWidth="750.0" style="-fx-background-color: #3D4956; -fx-border-width: 7; -fx-border-color: #6babd6;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.veltun.gui.rackStation.backend.backendController">
   <children>
      <Button fx:id="addS" layoutX="205.0" layoutY="187.0" mnemonicParsing="false" onAction="#switchToAdd" prefHeight="26.0" prefWidth="190.0" style="-fx-background-radius: 5em;" text="Add a station" />
      <Label layoutX="227.0" layoutY="120.0" prefHeight="43.0" prefWidth="190.0" text="Choose your action !" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="updateS" layoutX="205.0" layoutY="298.0" mnemonicParsing="false" onAction="#switchToUpdate" prefHeight="26.0" prefWidth="190.0" style="-fx-background-radius: 5em;" text="Update an existing station" />
      <Button fx:id="listS" layoutX="205.0" layoutY="242.0" mnemonicParsing="false" onAction="#switchToList" prefHeight="26.0" prefWidth="190.0" style="-fx-background-radius: 5em;" text="Show an existing stations" />
      <Button fx:id="crudS" cancelButton="true" layoutX="52.0" layoutY="340.0" mnemonicParsing="false" onAction="#switchToHomefromCRUDStation" prefHeight="25.0" prefWidth="103.0" style="-fx-background-radius: 5em; -fx-background-color: #FF6347;" text="Go Back" />
   </children>
</AnchorPane>
