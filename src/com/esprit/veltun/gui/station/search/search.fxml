<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"  xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.veltun.gui.station.search.StationSearchController">
   <children>
      <ListView fx:id="stationlistview" layoutX="18.0" layoutY="91.0" prefHeight="262.0" prefWidth="565.0" />
      <Button fx:id="removestationbutton" layoutX="385.0" layoutY="359.0" mnemonicParsing="false" onAction="#removeStation" prefHeight="25.0" prefWidth="80.0"  text="Supprimer" />
      <Button fx:id="editstationbutton" defaultButton="true" layoutX="485.0" layoutY="359.0" mnemonicParsing="false" onAction="#editStation" prefHeight="25.0" prefWidth="80.0" text="Modifier" />
      <TextField fx:id="titletosearch" layoutX="300.0" layoutY="56.0" prefHeight="25.0" prefWidth="195.0" promptText="Titre de recherche" />
      <Button fx:id="searchbutton" layoutX="501.0" layoutY="56.0" mnemonicParsing="false" onAction="#search" opacity="0.62"  text="Rechercher" />
      <ImageView fx:id="tfimagelogo" fitHeight="86.0" fitWidth="152.0" layoutY="3.0">
         <image>
            <Image url="http://localhost/logo-removebg-preview.png" />
         </image>
      </ImageView>
   </children>
   <stylesheets>

   </stylesheets>


</AnchorPane>
