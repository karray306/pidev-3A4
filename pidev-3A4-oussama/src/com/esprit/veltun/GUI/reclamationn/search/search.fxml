<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esprit.veltun.GUI.reclamationn.search.ReclamationSearchController">
   <children>
      <ListView fx:id="eventlistview" layoutX="18.0" layoutY="91.0" prefHeight="262.0" prefWidth="565.0" />
      <Button fx:id="removeeventbutton" layoutX="385.0" layoutY="359.0" mnemonicParsing="false" onAction="#removeEvent" prefHeight="25.0" prefWidth="80.0" text="Supprimer" />
      <Button fx:id="editeventbutton" defaultButton="true" layoutX="485.0" layoutY="359.0" mnemonicParsing="false" onAction="#editEvent" prefHeight="25.0" prefWidth="80.0" text="Modifier" />
      <Button fx:id="addneweventbutton" defaultButton="true" layoutX="287.0" layoutY="359.0" mnemonicParsing="false" onAction="#addEvent" prefHeight="25.0" prefWidth="80.0" text="Add " />
      <TextField fx:id="titletosearch" layoutX="300.0" layoutY="56.0" prefHeight="25.0" prefWidth="195.0" promptText="Titre de recherche" />
      <Button fx:id="searchbutton" layoutX="501.0" layoutY="56.0" mnemonicParsing="false" onAction="#search" opacity="0.62" text="Rechercher" />
      <Button fx:id="tributton" layoutX="74.0" layoutY="56.0" mnemonicParsing="false" onAction="#tri" prefHeight="25.0" prefWidth="54.0" text="tri" />
   </children>
</AnchorPane>
